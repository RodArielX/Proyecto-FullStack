{
    "version": 2,
    "builds": [
      {
        "src": "Frontend_Proyecto_DJ/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "Backend_Proyecto_DJ/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*).js",
        "dest": "/Backend_Proyecto_DJ/src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/Frontend_Proyecto_DJ/$1"
      }
    ]
  }